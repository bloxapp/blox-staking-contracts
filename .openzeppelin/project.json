{
  "manifestVersion": "2.2",
  "contracts": {
    "BloxStaking": "BloxStaking"
  },
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^3.0.0"
  },
  "name": "blox-staking-contracts",
  "version": "0.0.1",
  "compiler": {
    "compilerSettings": {
